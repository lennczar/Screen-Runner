{
	"manifest_version" : 2,
	"name" : "Screen Runner",
	"version" : "0.1",
	"content_scripts" : [
		{
			"matches" : [
				"<all_urls>"
			],
			"js" : [
				"p5.js",
				"p5.dom.js",
				"GUI.js",
				"screenrunner_screen.js",
				"player.js",
				"bullet.js",
				"screenrunner.js",
				"boxCollider.js"
			]
		}
	],
	"browser_action" : {
		"default_popup" : "popup.html"
	}
}